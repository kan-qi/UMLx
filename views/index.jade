  body
   include banner
   if message
    div.message-box
     p.message-text message:#{message}
   div.clear
   div(id='main-panel')
    include mainPanel
    // Modal
    #overlay-frame.modal.fade(role='dialog')
     .modal-dialog
        // Modal content
        .modal-content
            .modal-header
                button.close(type='button', data-dismiss='modal') &#x2715;
                h4.modal-title Modal Header
            .modal-body
                p Some text in the modal.
            .modal-footer
                button.btn.btn-default(type='button', onclick='fileDownload()') Download
                button.btn.btn-default(type='button', data-dismiss='modal') Close
    #dialog-frame.modal.fade(role='dialog')
     .modal-dialog
        // Modal content
        .modal-content
            .modal-header
                button.close(type='button', data-dismiss='modal') &#x2715;
                h4.modal-title Modal Header
            .modal-body
                p Some text in the modal.
            .modal-footer
                button.btn.btn-default(type='button', data-dismiss='modal') Close
