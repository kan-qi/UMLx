if diagram        
  div.use-case-info
     img.use-case(src=diagram.AccessDir+"/"+diagram.SvgGraphFile)
  div.use-case-info-content
   ul.nav.nav-tabs
    li.active
     a(data-toggle='tab', href='#use-case-nodes') Nodes
    li
     a(data-toggle='tab', href='#use-case-paths') Paths
   div.tab-content
    div.tab-pane.fade.in.active(id="use-case-nodes")
     table.table.table-striped.use-case-info-tab
      thead
       tr
        th ID
        th ELEMENT
        th TYPE
      tbody
       -var nodeId = 1;
       each node in diagram.Nodes
        tr
         th #{nodeId}
         th
          span.use-case-node #{node.Name}
         th
          span.use-case-node-type #{node.Type}
        - nodeId++;
    div.tab-pane.fade(id="use-case-paths")
     table.table.table-striped.use-case-info-tab
      thead
       tr
        th ID
        th PATH
        th TYPE
      tbody
       -var pathId = 1;
       each path in diagram.Paths
        tr
         th #{pathId}
         th
          span.use-case-path #{path.PathStr}
         th
          span.use-case-path-type #{path.Tag}
        - pathId++;