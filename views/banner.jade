div.banner
     div.content-flex
        div.col-md-10.banner-title
            a.home-button(href='/') UMLx
          ul.nav.nav-tabs.no-border.dropdown-style
            li.dropdown
                button.btn.btn-secondary.dropdown-toggle.menu-font.menu.btn-default(data-toggle='dropdown',role='button',aria-haspopup="true",aria-expanded="false") Menu
                    span.caret
                ul.dropdown-menu.dropdown-menu-right(style = "margin-top : 2px; width: 100px; border-radius : 5px;")
                    li.padding
                        a.dropdown-item.btn-menu.menu-font(role="tab", data-toggle="tab", href='#main_page' aria-expanded="true") Overview
                    li.padding
                        a.dropdown-item.btn-menu.menu-font(role="tab",data-toggle="tab", href='#repo_trending') Trending
                    li.padding
                        a.dropdown-item.btn-menu.menu-font(role="tab",data-toggle="tab", href='#project_list') Projects 
                            span(class="badge") #{modelAllNum}
                    li.padding
                        a.dropdown-item.btn-menu.menu-font(role="tab",data-toggle="tab", id='modelArchive', href='#repo_archive', data-url='#{path}', onclick='walkRepoDir(this)') Archives
                    li.padding
                        a.dropdown-item.btn-menu.menu-font(role="tab", data-toggle="tab", href='#profile') Profile
                    li.padding
                        a.dropdown-item.btn-menu.menu-font(role="tab", href='/logout') Log Out
    div.logo(style="float:left;vertical-align: center; width:100%;")
        img(src="img/logo&horizon.png" style="width:40%;display:block;margin:auto;")
    div.btns.ui.inverted.segment
        a.btn.btn-default(role="tab",data-toggle="tab",href="#main_page" aria-label="Skip to overview page")
            i.fa.fa-home.fa-2x(aria-hidden="true")

        a.btn.btn-default(role="tab",data-toggle="tab",href="#repo_trending" aria-label="Skip to trending page")
            i.fa.fa-bar-chart.fa-2x(aria-hidden="true")

        a.btn.btn-default(role="tab",data-toggle="tab",href="#project_list" aria-label="Skip to projects page")
            i.fa.fa-tasks.fa-2x(aria-hidden="true")
            span(class="badge") #{modelAllNum}
        a.btn.btn-default.btn4(role="tab",data-toggle="tab",href="#repo_archive" data-url='#{path}' onclick='walkRepoDir(this)' aria-label="Skip to trending page" )
            i.fa.fa-archive.fa-2x(aria-hidden="true")

