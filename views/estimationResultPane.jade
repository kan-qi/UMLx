if error
  p #{error}
if estimationResults
  div(id='estimation-results-tables')
    div.model-predictions
      table.table.table-bordered
        thead
          tr
            th Model
            th Effort (PH)
            th Schedule (Months)
            th #{estimationResults.SizeMetric}
            th Personnel (FT Developers)
            th UI Developer
            th Full Stack
            th Database Developer
        tbody
          tr.estimation-table
            td #{estimationResults.EstimationModel}
            td #{estimationResults.Effort}
            td #{estimationResults.Duration}
            td #{estimationResults.SizeMeasurement}
            td #{estimationResults.Personnel}
            td #{estimationResults.Personnel_UI}
            td #{estimationResults.Personnel_FS}
            td #{estimationResults.Personnel_DB}
  div(id='estimation-results-charts' data-url="#{modelInfo.AccessDir}/#{modelInfo.estimationResultsFile}")
    div(id='estimation-charts')
  div.use-case-predictions.table-responsive
    table.table.table-bordered
      thead
        tr
          th Number
          th Use Case
          th Effort (PH)
          th #{estimationResults.SizeMetric}
          th Schedule (Months)
          th Personnel (FT Developers)
          th UI Developer
          th Full Stack
          th Database Developer
      tbody
        - for (var i = 0; i < estimationResults.UseCases.length; i++) {
            tr.estimation-table
              td #{i}
              td #{estimationResults.UseCases[i].Name}
              td #{estimationResults.UseCases[i].Effort}
              td #{estimationResults.UseCases[i].SizeMeasurement}
              td #{estimationResults.UseCases[i].Duration}
              td #{estimationResults.UseCases[i].Personnel}
              td #{estimationResults.UseCases[i].Personnel_UI}
              td #{estimationResults.UseCases[i].Personnel_FS}
              td #{estimationResults.UseCases[i].Personnel_DB}
        - }

