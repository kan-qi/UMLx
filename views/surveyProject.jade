        function showPage(pageNumber) {
            var uuid = "";
            for (var i = 1; i <= 4; i++) {
                var uuidElementName = "uuid_page" + pageNumber;
                var classList = document.getElementById("page" + i).classList;
                if (i != pageNumber) {
                    classList.add("hidden");
                } else {
                    uuid = document.getElementById(uuidElementName).value;
                    classList.remove("hidden");
                }
            }
            send_analytics_data(uuid, ipAdress, pageNumber);
        }

        var ipAdress = "localhost--";

        document.addEventListener("DOMContentLoaded", function (event) {
            $.getJSON("https://api.ipify.org/?format=json", function (data) {
                //console.log("IP Address received");
                ipAdress = data.ip;
            });
        });

        function UUID() {
            var uuid = (function () {
                var i,
                    c = "89ab",
                    u = [];
                for (i = 0; i < 36; i += 1) {
                    u[i] = (Math.random() * 16 | 0).toString(16);
                }
                u[8] = u[13] = u[18] = u[23] = "-";
                u[14] = "4";
                u[19] = c.charAt(Math.random() * 4 | 0);
                return u.join("");
            })();
            return {
                toString: function () {
                    return uuid;
                },
                valueOf: function () {
                    return uuid;
                }
            };
        }



