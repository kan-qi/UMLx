doctype html
html(lang='en')
  head
    link(href='https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css', rel='stylesheet')
    script(type='text/javascript', src='http://code.jquery.com/jquery-1.9.1.min.js')
    script(type='text/javascript', src='https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js')
    script(type='text/javascript', src='js/scripts.js')
    script(type='text/javascript', src='js/scripts.js')
    link(href='css/style.css', rel='stylesheet')
    title Estimation Request List
  body
    include banner
    h1 Estimation Request List
    #container.col
     if estimationRequests
      table
       thead
         tr
          th Creation Time
          th Package
       tbody
        each estimationRequest in estimationRequests
         tr
          td #{estimationRequest.creationTime}
          td
           a(href='/getZipPackage?repo_id=#{estimationRequest.repo_id}&model_id=#{estimationRequest.fileId}') Download Package